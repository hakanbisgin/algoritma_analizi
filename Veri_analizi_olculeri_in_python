vary = varyans(sap)
max = maxBul(dizi)
min = minBul(dizi)
ort = ortalama(dizi)
print("Sapma : ", sap)
print("Varyans : ", vary)
print("Max : ", max)
print("Min : ", min)
print("Ortalama : ", ort)
